<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@+id/selected"
		android:drawable="@drawable/ic_people_fill"
		android:state_checked="true"/>

	<item
		android:id="@+id/unselected"
		android:drawable="@drawable/ic_people_outline"
		android:state_checked="false"/>

	<transition
		android:fromId="@id/selected"
		android:toId="@id/unselected"
		android:reversible="true"
		android:drawable="@drawable/ic_people_fill_to_outline"/>
</animated-selector>
