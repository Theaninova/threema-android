<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@+id/selected"
		android:drawable="@drawable/ic_chat_bubble"
		android:state_checked="true"/>

	<item
		android:id="@+id/unselected"
		android:drawable="@drawable/ic_about_outline"
		android:state_checked="false"/>

	<transition
		android:fromId="@id/unselected"
		android:toId="@id/selected"
		android:reversible="true"
		android:drawable="@drawable/transition_ic_chat_bubble_unselected_to_selected"/>
</animated-selector>
