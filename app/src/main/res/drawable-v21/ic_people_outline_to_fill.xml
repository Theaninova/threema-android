<animated-vector
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aapt="http://schemas.android.com/aapt"
	android:drawable="@drawable/ic_people_outline">
	<target android:name="fill">
		<aapt:attr name="android:animation">
			<objectAnimator
				android:duration="200"
				android:interpolator="@android:anim/accelerate_decelerate_interpolator"
				android:propertyName="translateY"
				android:valueFrom="9.5"
				android:valueTo="0"
				android:valueType="floatType" />
		</aapt:attr>
	</target>
	<target android:name="heads">
		<aapt:attr name="android:animation">
			<set>
				<objectAnimator
					android:duration="200"
					android:interpolator="@android:anim/accelerate_decelerate_interpolator"
					android:propertyName="strokeWidth"
					android:valueFrom="1.5"
					android:valueTo="3"
					android:valueType="floatType" />
				<objectAnimator
					android:duration="200"
					android:interpolator="@android:anim/accelerate_decelerate_interpolator"
					android:propertyName="pathData"
					android:valueFrom="
						M7.5 11.5a2.5,2.5 0 1 1 0.1,0Z
                        M16.5 11.5a2.5,2.5 0 1 1 0.1,0Z"
					android:valueTo="
						M7.5 10.5a1.5,1.5 0 1 1 0.1,0Z
                        M16.5 10.5a1.5,1.5 0 1 1 0.1,0Z"
					android:valueType="pathType" />
			</set>
		</aapt:attr>
	</target>
</animated-vector>
